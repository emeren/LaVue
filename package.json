        "cross-env": "^7.0",
        "cypress": "^4.3.0",
        "expect": "^25.2.7",
        "faker": "^4.1.0",
        "jquery": "^3.2",
        "jsdom": "^16.2.2",
        "jsdom-global": "^3.0.2",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.13",
        "mocha": "^7.1.1",
        "mochapack": "^1.1.13",
        "popper.js": "^1.12",
